{
  "name": "OpenVim",
  "version": "0.0.4",
  "publisher": "mattn",
  "description": "Open a current file in Vim",
  "author": {
    "name": "Yasuhiro Matsumoto (a.k.a mattn)"
  },
	"icon": "images/vim.png",
  "categories": ["Other"],
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mattn/vscode-openvim"
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "engines": {
    "vscode": "0.10.x"
  },
  "main": "./out/extension",
  "extensionDependencies": [],
  "activationEvents": [
    "onCommand:extension.openVim"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.openVim",
        "title": "Open Vim",
        "description": "Open current file with Vib"
      }
    ]
  },
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/mattn/vscode-openvim/issues"
  }
}
